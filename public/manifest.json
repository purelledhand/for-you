{
  "manifest_version": 2,
  "name": "For you",
  "description": "Explore windows around the world with new tab",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": [],

  "content_security_policy": "script-src 'self’ ‘unsafe-eval'; object-src 'self'",

  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}